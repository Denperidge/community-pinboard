{
  "name": "community-pinboard",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev",
    "build:ts": "tsc",
    "build:assets": "cp --parents -r {public/**/*,views/*} dist/",
    "build": "yarn build:ts && yarn build:assets",
    "prod": "set NODE_ENV=production & cd dist && node ./bin/www",
    "dev": "ts-node-dev --respawn ./bin/www",
    "dev:debug": "set DEBUG=community-pinboard:* & yarn dev"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.19.2",
    "express-validator": "^7.0.1",
    "http-errors": "~1.6.3",
    "ics": "^3.7.2",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-sass-middleware": "^1.1.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  }
}
